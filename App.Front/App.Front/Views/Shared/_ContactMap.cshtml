@model App.Domain.Entities.GlobalSetting.ContactInfomation
<div class="google-map">
    @if (Model != null)
    {
        <script type="text/javascript">
        function ShowMap() {
            initMap('@Model.Lat',
                '@Model.Lag',
                '<h2>@Model.Title<h2><p>Hotline: @Model.Hotline</p><p>Điện thoại: @Model.MobilePhone</p><p>@Model.Email</p><p>Địa chỉ: @Model.Address</p>',
                'map_canvas');
        }

        $(function() {
            ShowMap();
        })
        </script>
    }
    <div id="map_canvas" class="map_canvas"></div>
</div>
