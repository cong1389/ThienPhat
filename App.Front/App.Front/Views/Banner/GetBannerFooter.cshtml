@using App.Utils
@model IEnumerable<App.Domain.Entities.Ads.Banner>
@if (Model.IsAny())
{
    <div class="footer-banner">
        @foreach (var banner in Model)
        {
            <a href="@banner.WebsiteLink" target="@banner.Target" rel="nofollow">
                <img src="@Url.Content("~/" + banner.ImgPath)"/>
            </a>
        }
    </div>
}
