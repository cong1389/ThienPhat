@using App.Utils
@model IEnumerable<App.Domain.Entities.Ads.Banner>
@if (Model.IsAny())
{
    <div class="rbanner ">
        @foreach (var item in Model)
        {
            <a href="@item.WebsiteLink" rel="nofollow " target="@item.Target" class="img-reponsive">
                <img src="@Url.Content("~/"+item.ImgPath)" />
            </a>
        }
    </div>
}
