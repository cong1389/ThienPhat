@using App.Domain.Common
@using Resources
@model App.FakeEntity.GenericControl.GenericControlViewModel
@{
    ViewBag.Title = UI.CreateNew;
    ViewBag.Toolbar = new[] { "Save", "Divider", "Back" };
}
@using (Html.BeginForm("Edit", "GenericControl", FormMethod.Post, new { data_container = ViewContext.RouteData.Values["controller"], enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(x => x.Id)

    Html.RenderPartial("_CreateOrUpdate", Model);
}



